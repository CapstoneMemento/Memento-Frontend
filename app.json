{
  "expo": {
    "name": "memento-frontend",
    "slug": "memento-frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yyeonkim.mementofrontend"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.yyeonkim.mementofrontend"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "plugins": [
      "react-native-document-scanner-plugin"
    ],
    "extra": {
      "eas": {
        "projectId": "85d3bb10-9878-43db-904a-9bb6583257ac"
      }
    }
  }
}
